{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "paths": {
      "$elements/*": [
        "components/elements/*"
      ],
      "$parts/*": [
        "components/parts/*"
      ],
      "$lib/*": [
        "lib/*"
      ],
      "~common/*": [
        "../common/*"
      ]
    },
    "baseUrl": "./",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": [
      "vite/client"
    ]
  }
}
