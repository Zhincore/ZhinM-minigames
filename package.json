{
  "name": "zhinm-gui",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently -c green,blue npm:dev:nui npm:dev:game",
    "dev:game": "nodemon -e js,cjs,mjs,ts,tsx -w ./src -i ./src/nui --exec spack",
    "dev:nui": "vite",
    "build": "concurrently -c green,blue npm:build:nui npm:build:game",
    "build:game": "spack",
    "build:nui": "vite build",
    "serve": "vite preview",
    "lint": "eslint --fix src",
    "format": "prettier --write src"
  },
  "license": "MIT",
  "devDependencies": {
    "@citizenfx/client": "^2.0.6253-1",
    "@citizenfx/server": "^2.0.6253-1",
